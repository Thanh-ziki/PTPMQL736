@model IEnumerable<PTPMQL.Models.Meeting>

@{
    ViewData["Title"] = "Index";
}

<h1>Báo cáo hệ thống</h1>

<p>
    <a asp-action="Create">thêm báo cáo mới</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ChuDeHop)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaChuyenBay)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NoiDungHop)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ChuDeHop)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaChuyenBay)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoiDungHop)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IDNhanVien">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IDNhanVien">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IDNhanVien">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
