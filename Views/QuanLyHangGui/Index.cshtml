@model IEnumerable<PTPMQL.Models.QuanLiHangGui>

@{
    ViewData["Title"] = "Index";
}

<h1>Ký gửi</h1>

<p>
    <a asp-action="Create">Thông tin gửi hàng</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NgayGioGuiHang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrongLuongHangGui)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.XacNhanHopLe)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NgayGioGuiHang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrongLuongHangGui)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.XacNhanHopLe)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaHangGui">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaHangGui">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaHangGui">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
